{
    "type": "mongodb",
    "host": "localhost",
    "database": "test",
    "synchronize": true,
    "logging": false,
    "entities": [
        "./dist/**/**/*.entity{.ts,.js}",
        "./src/**/**/*.entity{.ts,.js}"
    ],
    "migrations": [
        "src/database/migrations/*{.ts,.js}"
    ],
    "cli": {
        "migrationsDir": "src/database/migrations"
    }
}